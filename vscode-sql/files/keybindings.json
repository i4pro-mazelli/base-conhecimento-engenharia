[
  {
    "key": "f5",
    "command": "mssql.runCurrentStatement",
    "when": "editorTextFocus && editorLangId == 'sql'"
  },
  {
    "key": "f5",
    "command": "mssql.runQuery",
    "when": "editorTextFocus && editorLangId == 'sql'"
  },
  {
    "key": "ctrl+e",
    "command": "workbench.action.closePanel",
    "when": "editorTextFocus && editorLangId == 'sql'"
  },
  {
    "key": "ctrl+r",
    "command": "mssql.revealQueryResultPanel",
    "when": "editorTextFocus && editorLangId == 'sql'"
