  {
    "key": "ctrl+f1",
    "command": "multiCommand.runSpHelpText",
    "when": "editorTextFocus && editorLangId == 'sql'"
  },
  {
    "key": "ctrl+3",
    "command": "multiCommand.runSpSelect",
    "when": "editorTextFocus && editorLangId == 'sql'"
